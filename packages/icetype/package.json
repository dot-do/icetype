{
  "name": "icetype",
  "version": "0.3.0",
  "description": "IceType - Type-safe schema language for data lakes and databases",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "ice": "./dist/cli.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./core": {
      "types": "./dist/core.d.ts",
      "import": "./dist/core.js"
    },
    "./iceberg": {
      "types": "./dist/iceberg.d.ts",
      "import": "./dist/iceberg.js"
    },
    "./adapters": {
      "types": "./dist/adapters.d.ts",
      "import": "./dist/adapters.js"
    },
    "./postgres": {
      "types": "./dist/postgres.d.ts",
      "import": "./dist/postgres.js"
    },
    "./mysql": {
      "types": "./dist/mysql.d.ts",
      "import": "./dist/mysql.js"
    },
    "./sqlite": {
      "types": "./dist/sqlite.d.ts",
      "import": "./dist/sqlite.js"
    },
    "./drizzle": {
      "types": "./dist/drizzle.d.ts",
      "import": "./dist/drizzle.js"
    },
    "./prisma": {
      "types": "./dist/prisma.d.ts",
      "import": "./dist/prisma.js"
    },
    "./clickhouse": {
      "types": "./dist/clickhouse.d.ts",
      "import": "./dist/clickhouse.js"
    },
    "./duckdb": {
      "types": "./dist/duckdb.d.ts",
      "import": "./dist/duckdb.js"
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "icetype",
    "schema",
    "typescript",
    "iceberg",
    "parquet",
    "data-lake",
    "type-safe"
  ],
  "author": "Nathan Clevenger",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/dot-do/icetype.git"
  },
  "homepage": "https://github.com/dot-do/icetype",
  "bugs": {
    "url": "https://github.com/dot-do/icetype/issues"
  },
  "dependencies": {
    "@icetype/core": "workspace:*",
    "@icetype/iceberg": "workspace:*",
    "@icetype/cli": "workspace:*",
    "@icetype/adapters": "workspace:*",
    "@icetype/clickhouse": "workspace:*",
    "@icetype/duckdb": "workspace:*",
    "@icetype/drizzle": "workspace:*",
    "@icetype/mysql": "workspace:*",
    "@icetype/postgres": "workspace:*",
    "@icetype/prisma": "workspace:*",
    "@icetype/sqlite": "workspace:*"
  },
  "devDependencies": {
    "typescript": "^5.7.0",
    "vitest": "^2.1.0"
  }
}
