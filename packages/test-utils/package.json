{
  "name": "@icetype/test-utils",
  "version": "0.1.0",
  "description": "Shared test utilities and schema factories for IceType",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "icetype",
    "testing",
    "test-utils",
    "schema-factories"
  ],
  "author": "Nathan Clevenger",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/dot-do/icetype.git"
  },
  "homepage": "https://github.com/dot-do/icetype",
  "bugs": {
    "url": "https://github.com/dot-do/icetype/issues"
  },
  "dependencies": {
    "@icetype/core": "workspace:*"
  },
  "devDependencies": {
    "typescript": "^5.7.0",
    "vitest": "^2.1.0"
  }
}
