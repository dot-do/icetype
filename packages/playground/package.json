{
  "name": "@icetype/playground",
  "version": "0.1.0",
  "description": "Interactive web playground for IceType schemas",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "dev": "vite",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "icetype",
    "playground",
    "schema",
    "editor",
    "typescript"
  ],
  "author": "Nathan Clevenger",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/dot-do/icetype.git"
  },
  "homepage": "https://github.com/dot-do/icetype",
  "bugs": {
    "url": "https://github.com/dot-do/icetype/issues"
  },
  "dependencies": {
    "@icetype/core": "workspace:*",
    "@icetype/postgres": "workspace:*",
    "@icetype/mysql": "workspace:*",
    "@icetype/sqlite": "workspace:*",
    "preact": "^10.19.0"
  },
  "devDependencies": {
    "@preact/preset-vite": "^2.8.0",
    "@testing-library/preact": "^3.2.0",
    "jsdom": "^24.0.0",
    "typescript": "^5.7.0",
    "vite": "^5.0.0",
    "vitest": "^2.1.0"
  }
}
