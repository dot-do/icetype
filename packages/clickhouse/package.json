  "keywords": [
    "icetype",
    "clickhouse",
    "adapter",
    "schema",
    "ddl",
    "typescript"
  ],
  "author": "Nathan Clevenger",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/dot-do/icetype.git"
  },
  "homepage": "https://github.com/dot-do/icetype",
  "bugs": {
    "url": "https://github.com/dot-do/icetype/issues"
  },
  "dependencies": {
    "@icetype/core": "workspace:*",
    "@icetype/sql-common": "workspace:*"
  },
  "devDependencies": {
    "typescript": "^5.7.0",
    "vitest": "^2.1.0"
  }
}
